    "#Take a stratified 10% subsample of the data. \n",
    "#Plot the distribution of the classes (as a histogram) for both the original data and the subsample. \n",
    "SVHN = oml.datasets.get_dataset(41081)                                   #Load data\n",
    "X, y, cats, attrs = SVHN.get_data(dataset_format='array',\n",
    "    target=SVHN.default_target_attribute)\n",
    "\n",
    "sss = StratifiedShuffleSplit(n_splits=10, test_size=0.2, random_state=0) #Use StratifiedShuffleSplit class in a \"clever\" (unintended way) to generate stratified sub-sample.\n",
    "train_index, test_index = next(sss.split(X, y))                          #Generate indices of the first train/test split (Since sss.split(X, y) is a generator we use next here to generate the first split)\n",
    "y_complete = np.concatenate((y[train_index], y[test_index]))             #Concatenate the train/test data for the sake of the Histogram plot (So we can see full distribution). We do this for both X and y,\n",
    "X_complete = np.concatenate((X[train_index], X[test_index]))             #Since we need the stratified sub-sample for later questions. \n",
    "print(len(y_complete), len(X_complete))\n",
    "\n",
    "\n",
    "plt.hist(y_complete, bins=10, histtype='bar', edgecolor='black')         #Histogram of Sub-sample\n",
    "plt.title(\"Distribution of sub-sample Y\")\n",
    "plt.xlabel(\"Different classes (0-9, 0 is coded as 10, 1 is coded as 1) \")\n",
    "plt.ylabel(\"Count\")\n",
    "\n",
    "plt.hist(y, bins=10, histtype='bar', edgecolor='black')                 #Histogram of original data\n",
    "plt.title(\"Distribution of sub-sample Y\")\n",
